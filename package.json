{
  "name": "@rowboat/adonis-bull",
  "description": "The easiest way to start using an asynchronous job queue with AdonisJS. Supports version 5 and 4.x",
  "version": "1.0.4",
  "main": "build/providers/BullProvider.js",
  "types": "build/adonis-typings/index.d.ts",
  "license": "MIT",
  "scripts": {
    "test": "node japaFile.js",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm run test",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "compile": "tsc",
    "prebuild": "rimraf ./build",
    "build": "npm run compile && npm run copyfiles",
    "lint": "eslint --ignore-path .gitignore",
    "format": "prettier \"**/*.ts\" --write --ignore-path .gitignore",
    "prepublishOnly": "npm run build",
    "publish:alpha": "np --branch=alpha --tag=alpha"
  },
  "np": {
    "yarn": false,
    "contents": ".",
    "anyBranch": false
  },
  "publishConfig": {
    "access": "public"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "directories": {
    "test": "test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "commitlint": {
    "extends": [
      "@rocketseat/commitlint-config"
    ]
  },
  "keywords": [
    "bull",
    "queue",
    "adonis",
    "adonisjs",
    "adonis-js",
    "adonis-bull",
    "adonis-queue"
  ],
  "bugs": {
    "url": "https://github.com/rocketseat/adonis-bull/issues"
  },
  "homepage": "https://github.com/rocketseat/adonis-bull#readme",
  "devDependencies": {
    "@adonisjs/ace": "^11.3.1",
    "@adonisjs/application": "^5.2.5",
    "@adonisjs/core": "^5.8.5",
    "@adonisjs/fold": "^8.2.0",
    "@adonisjs/logger": "^4.1.4",
    "@adonisjs/mrm-preset": "^5.0.3",
    "@adonisjs/sink": "^5.4.0",
    "@commitlint/cli": "^17.0.3",
    "@poppinss/dev-utils": "^2.0.3",
    "@rocketseat/commitlint-config": "^0.0.3",
    "@types/node": "^18.6.1",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "copyfiles": "^2.4.1",
    "coveralls": "^3.1.1",
    "delay": "^5.0.0",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-standard": "^4.1.0",
    "husky": "^8.0.1",
    "japa": "^4.0.0",
    "lint-staged": "^13.0.3",
    "np": "^7.6.2",
    "nyc": "^15.1.0",
    "prettier": "^2.7.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rocketseat/adonis-bull.git"
  },
  "peerDependencies": {
    "@adonisjs/core": "^5.0.4-preview-rc"
  },
  "dependencies": {
    "@bull-board/express": "^4.0.1",
    "bull-board": "^2.0.3",
    "bullmq": "^1.86.7"
  },
  "files": [
    "build/adonis-typings",
    "build/commands",
    "build/providers",
    "build/src",
    "build/templates",
    "build/instructions.js",
    "build/instructions.d.ts"
  ],
  "typings": "build/adonis-typings/index.d.ts",
  "adonisjs": {
    "instructions": "./build/instructions.js",
    "types": "@rowboat/adonis-bull",
    "providers": [
      "@rowboat/adonis-bull"
    ],
    "commands": [
      "@rowboat/adonis-bull/build/commands"
    ]
  }
}
